{
  "name": "rboschin/dockernator-revamped",
  "type": "library",
  "description": "PHP port of name generator used by docker to generate container names.",
  "keywords": [
    "generator",
    "names",
    "names generator",
    "docker names",
    "container name"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Roberto Boschin",
      "email": "rboschin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rboschin\\DockernatorRevamped\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rboschin\\DockernatorRevamped\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^8.0|^9.0|^10.0",
    "symfony/var-dumper": "~5.0"
  },
  "config": {
    "preferred-install": "dist"
  }
}
